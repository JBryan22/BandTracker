@{
    Layout = "_Layout";
}
<h1 class="header">Venues Being Tracked:</h1>

<div class="container">
    @if (@Model.Count == 0 || @Model == null)
    {
        <h3 class="header">There are currently no venues being tracked!</h3>
    }
    else
    {
         <div class="well">
            <ul>
                @foreach(var venue in @Model)
                {
                    <li class="list"><a href="/venue/@venue.GetId()">@venue.GetName()</a></li>
                }
            </ul>
         </div>
    }
</div>