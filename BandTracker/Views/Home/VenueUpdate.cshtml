@{
    Layout = "_Layout";
}

<h1 class="header">Update @Model.GetName() Name:</h1>

<div class="container">
    <form action="/venues/@Model.GetId()/update" method="post" class="form-group">
        <label for="venue-name">New Name:</label>
        <input type="text" name="venue-name" class="form-control">
        <button type="submit" class="btn btn-default form-submit">Submit</button>
    </form>
</div>